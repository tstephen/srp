    <serviceTask flowable:type="http" completionQuantity="1" id="setReturnStatus" implementation="flowable:http" isForCompensation="false" name="Set return status&#10;to submitted" startQuantity="1">
      <extensionElements>
        <flowable:field name="requestMethod" stringValue="POST"/>
        <flowable:field name="requestUrl" expression="https://v3.srp.digital/returns/${returnId}/status/Submitted"/>
        <flowable:field name="requestHeaders">
          <flowable:expression>Content-Type:application/json
Accept:application/json
X-RunAs:${initiator}</flowable:expression>
        </flowable:field>
        <!-- TODO flowable:http cannot (yet) handle OAuth / JWT -->
        <flowable:field name="resourceUsername" stringValue="userInfo('tenant-bot')"/>
        <flowable:field name="resourcePassword" expression="userInfo('cust-mgmt-secret')"/>
        <flowable:field name="jwtLoginResource" expression="userInfo('jwt-login-url')"/>
      </extensionElements>
      <incoming>_4</incoming>
