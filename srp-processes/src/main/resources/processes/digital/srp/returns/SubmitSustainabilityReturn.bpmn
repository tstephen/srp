    <callActivity calledElement="SendAlert" completionQuantity="1" id="alertNewReturn" isForCompensation="false" name="Alert new return submitted" startQuantity="1">
      <extensionElements>
        <flowable:in sourceExpression="SRP: New return for ${org} ${applicablePeriod} is available" target="subject"/>
        <flowable:in sourceExpression="tim@knowprocess.com" target="addressee"/>
        <flowable:in sourceExpression="SRP: &lt;a href=https://srp.digital/accounts.html?q=${org}>Click to view&lt;/a>" target="message"/>
        <flowable:in sourceExpression="${tenantId}" target="tenantId"/>
      </extensionElements>
      <incoming>_5</incoming>
      <outgoing>_10</outgoing>
      <ioSpecification>
        <dataInput id="DinalertNewReturn_8" isCollection="false" name="submitter"/>
